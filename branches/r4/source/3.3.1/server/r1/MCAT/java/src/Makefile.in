include ../../../mk/mk.config

# If javaDir is defined (via configure), then use the javac under that
# directory, otherwise use just "javac" to use the one in the path (if
# there is one).
ifdef $(javaDir)
myjavac = $(javaDir)/bin/javac
else
# Either just 'javac' or
# JAVADIR/javac if the environment variable JAVADIR is defined
myjavac = $(JAVADIR)javac
endif



.SUFFIXES: .java .class

CLASSDIR = ../../../jargon/bin  # this was ../../../java/classes but now use Jargon
THIS_CLASSDIR = ../classes
THIS_CLASSDIR_LONG = ../classes/edu/sdsc/SrbAdmin
THIS_CLASSDIR2_LONG = ../classes/edu/sdsc/SrbBrowser

myCLASSPATH = $(THIS_CLASSDIR):$(CLASSDIR)

# TARGETS

TARGETS:= $(THIS_CLASSDIR_LONG)/mcatAdmin.class

LIB_CLASS = \
$(THIS_CLASSDIR2_LONG)/SrbElement.class\
$(THIS_CLASSDIR2_LONG)/SrbCellRenderer.class\
$(THIS_CLASSDIR_LONG)/mcatNewUser.class  	\
$(THIS_CLASSDIR_LONG)/mcatNewLoc.class $(THIS_CLASSDIR_LONG)/mcatNewToken.class \
$(THIS_CLASSDIR_LONG)/mcatNewResource.class  \
$(THIS_CLASSDIR_LONG)/mcatNewLogResc.class $(THIS_CLASSDIR_LONG)/mcatAddLogResc.class \
$(THIS_CLASSDIR_LONG)/mcatNewGroup.class \
$(THIS_CLASSDIR_LONG)/mcatModUser.class \
$(THIS_CLASSDIR_LONG)/mcatNewCompoundResc.class \
$(THIS_CLASSDIR_LONG)/mcatAddToCompResc.class \
$(THIS_CLASSDIR_LONG)/mcatDelUser.class \
$(THIS_CLASSDIR_LONG)/mcatDelResc.class \
$(THIS_CLASSDIR_LONG)/mcatDelLoc.class \
$(THIS_CLASSDIR_LONG)/mcatNewDomain.class \
$(THIS_CLASSDIR_LONG)/mcatShowZonesDetail.class \
$(THIS_CLASSDIR_LONG)/mcatShowZones.class \
$(THIS_CLASSDIR_LONG)/mcatModZone.class \
$(THIS_CLASSDIR_LONG)/mcatNewZone.class \
$(THIS_CLASSDIR_LONG)/mcatDelZone.class \
$(THIS_CLASSDIR_LONG)/mcatShowRescsDetail.class \
$(THIS_CLASSDIR_LONG)/mcatShowRescs.class \
$(THIS_CLASSDIR_LONG)/mcatDelLogResc.class \
$(THIS_CLASSDIR_LONG)/mcatShowTokens.class \
$(THIS_CLASSDIR_LONG)/mcatShowUsers.class \
$(THIS_CLASSDIR_LONG)/mcatRmFromLogResc.class \
$(THIS_CLASSDIR_LONG)/mcatModUserGroup.class \
$(THIS_CLASSDIR_LONG)/mcatConnect.class \


all: $(TARGETS)

clean:
	rm -f $(THIS_CLASSDIR_LONG)/*.class
	rm -f $(THIS_CLASSDIR2_LONG)/*.class

# DEPENDENCIES

$(THIS_CLASSDIR_LONG)/mcatAdmin.class: mcatAdmin.java mcatConnect.java $(LIB_CLASS)
	$(myjavac) -g -classpath $(myCLASSPATH) -d $(THIS_CLASSDIR) $<

