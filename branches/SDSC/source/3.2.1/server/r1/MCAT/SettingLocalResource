
setenv srbUser srb
setenv mdasDomainName sdsc
setenv srbAuth CANDO
bin/ingestToken Domain lter gen-lvl4
bin/ingestUser lteradmin LTERADMIN lter sysadmin '' '' ''
bin/ingestLocation 'lorax lternet' 'lorax.lternet.edu:NULL:NULL' 'level4' lteradmin lter 
bin/ingestResource 'lorax-unix' 'unix file system' 'lorax lternet' '/tmp/SRBVault/?USER.?DOMAIN/?SPLITPATH/TEST.?PATH?DATANAME.?RANDOM.?TIMESEC' permanent 0

bin/ingestLocation  sdsc 'sdsc.edu:NULL:NULL' 'level4' srb sdsc
bin/ingestLocation  'catharsis sdsc' 'catharsis.sdsc.edu:NULL:NULL' 'level4' srb sdsc
bin/ingestResource 'unix-catharsis-sdsc'  'unix file system' 'catharsis sdsc' '/tmp/SRBVault/?USER.?DOMAIN/?SPLITPATH/TEST.?PATH?DATANAME.?RANDOM.?TIMESEC' permanent 0

bin/ingestLocation  'catharsis oracle Vault'  'catharsis.sdsc.edu:dlib1:foo' 'catharsis sdsc' srb sdsc
bin/ingestResource 'ora-catharsis-sdsc' 'oracle dblobj database' 'catharsis oracle Vault'  "/srbVault/OBJ_LOCATOR/OBJ_ID='TEST.?PATH/?DATANAME.?RANDOM.?TIMESEC'" permanent 0

bin/ingestLocation 'catharsis dlib1 DB' 'catharsis.sdsc.edu:dlib1:foo' 'catharsis sdsc' srb sdsc
bin/ingestResource 'ora-dlib1-sdsc'  'oracle table database' 'catharsis dlib1 DB' '' permanent 0

bin/ingestLocation 'hpss17 sdsc' 'hpss17.sdsc.edu:NULL:NULL' 'sdsc' srb sdsc
bin/ingestLocation 'hpss17 db2 mcat' 'hpss17.sdsc.edu:mcat:db2v5'  'hpss17 sdsc'  srb sdsc
bin/ingestResource 'db2-hpss17' 'db2 dblobj database' 'hpss17 db2 mcat'  "/srb.srbVault/OBJ_LOCATOR/OBJ_ID='?PATH/?DATANAME.?RANDOM.?TIMESEC'" permanent 0

bin/ingestResource 'db2-mcat-hpss17' 'db2 table database' 'hpss17 db2 mcat' '' permanent 0

CREATE TABLE srbVault ( obj_id             varchar(250),
                        obj_locator       long,
                             primary key (obj_id)
);


