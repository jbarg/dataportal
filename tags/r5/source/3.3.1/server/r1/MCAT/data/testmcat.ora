#!/bin/csh
source $HOME/.cshrc
rm data/mytest.results.ora
echo "testing started - this may take a few minutes"
cd bin
test.catalog >& ../data/mytest.results.ora
cd ..
diff data/mytest.results.ora data/test.results.ora
wc data/*test.results.ora
echo "testing completed. Successful if diff and wc results above are identical."
echo "installation and testing completed."


