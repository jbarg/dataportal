alias cd 'set myinp = \!*; set myfs = LOCAL_FS; if ($myinp =~ /srb/* || ($CWD_FS_FLAG == SRB_FS && $myinp !~ /* )) set myfs = SRB_FS; if ($myfs == SRB_FS ) $SrbUtilDir/Scd $myinp; set mystatus = $status;if ($myfs == SRB_FS && $mystatus == 0 ) setenv SRB_CWD /srb`$SrbUtilDir/Spwd`; if ($myfs == SRB_FS  && $mystatus == 0 ) setenv CWD_FS_FLAG SRB_FS; if ( $myfs == SRB_FS && $mystatus == 0 ) set prompt = SRB-\!\%-; if ($myfs == LOCAL_FS ) cd $myinp; set mystatus = $status; if ($myfs == LOCAL_FS && $mystatus == 0 ) setenv LOCAL_CWD $PWD; if ($myfs == LOCAL_FS && $mystatus == 0 ) setenv CWD_FS_FLAG LOCAL_FS; if ($myfs == LOCAL_FS && $mystatus == 0 ) set prompt = local-\!\%-;echo `pwd`;'
alias ss 'setenv CWD_FS_FLAG SRB_FS; $SrbUtilDir/Scd $SRB_CWD; set prompt = SRB-\!\%-;echo $SRB_CWD'
alias sl 'setenv CWD_FS_FLAG LOCAL_FS;set prompt = local-\!\%-;echo $LOCAL_CWD;'
