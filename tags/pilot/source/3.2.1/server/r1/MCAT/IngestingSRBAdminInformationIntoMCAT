setenv srbUser srb
setenv mdasDomainName sdsc
setenv srbAuth CANDO
bin/ingestToken Domain 'YOURDOMAIN'  gen-lvl4
bin/ingestUser 'SRBADMINNAME' 'SRBADMINPASSWORD' 'YOURDOMAIN' sysadmin 'SRBADMINADDR' 'SRBADMINPHONE' 'SRBADMINEMAIL'
bin/ingestLocation  'LOCNNAME' 'LOCATIONIPADDRESS:NULL:NULL' 'level4' 'SRBADMINNAME' 'YOURDOMAIN'
bin/ingestResource 'RESOURCENAME' 'unix file system'  'LOCNNAME'  'PATHTOSRBVAULT/SRBVault/?USER.?DOMAIN/?SPLITPATH/TEST.?PATH?DATANAME.?RANDOM.?TIMESEC' permanent 0
mkdir PATHTOSRBVAULT/SRBVault
chmod 700  PATHTOSRBVAULT/SRBVault
unsetenv srbUser
unsetenv mdasDomainName
unsetenv srbAuth



